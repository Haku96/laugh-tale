{
  "compilerOptions": {
    "target": "es2021",
    "lib": ["es2021", "DOM"],
    "module": "es2022",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "skipLibCheck": true
  },
  "include": ["**/*.ts", "**/*.d.ts", "**/*.vue"]
}
